<section class="pagination">
  <% if (hasPreviousPage) { %>
      <a href="?page=<%= currentPage - 1 %>">previousPage</a>
  <% } %> 
  <% for(let i = 1; i <= totalPage; i++) { %>
      <a href="?page=<%= i %>" class="<%= currentPage === i ? 'active': '' %>" ><%= i %></a>
  <% } %> 
  <% if (hasNextPage) { %>
      <a href="?page=<%= currentPage + 1 %>">Next</a>
  <% } %>
</section>